server {
	listen 80;
	listen [::]:80;
	listen 443 ssl;
	listen [::]:443 ssl;

	root /var/www/html/public;

	index index.html;

	server_name csci430.campbellcrowley.com;

	location / {
		try_files $uri $uri/ =404;
	}
  location /api {
    proxy_pass http://localhost:8080/$uri;
    proxy_set_header HOST $host;
  }
}
